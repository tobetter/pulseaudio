#!/bin/sh

case "$1" in
  install|upgrade)
    if dpkg --compare-versions "$2" lt "0.9.13-1ubuntu1"; then
      if [ -f /etc/X11/Xsession.d/70pulseaudio ]; then
        rm /etc/X11/Xsession.d/70pulseaudio
      fi
    fi
  ;;
  abort-upgrade)
  ;;
  *)
    echo "preinst called with unknown argument \`$1'" >&2
    exit 1
  ;;
esac

#DEBHELPER#

exit 0
